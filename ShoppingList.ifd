{
  "kind": "discovery#restDescription",
  "discoveryVersion": "v1",
  "id": "ShoppingListjs:v1",
  "name": "ShoppingList",
  "version": "v2.0",
  "title": "Shopping List",
  "if.category": "Shopping List",
  "protocol": "js",
  "basePath": "ShoppingList",
  "if.dependencies": [
    "EventEmitter.js",
    "lodash.min.js",
    "ShoppingList.js",
    "ListItem.js"
  ],
  "icons": {
    "x32": "Icon-ShoppingList.png"
  },
  "schemas": {
    "ShoppingList": {
      "id": "ShoppingList",
      "type": "object",
      "description": "ShoppingList",
      "properties": {
        "listItems": {
          "title": "List Items",
          "type": "array",
          "readonly": true,
          "items": {
            "$ref": "ListItem"
          }
        },
        "designModeItems": {
          "title": "List Items - Design Mode",
          "type": "array",
          "readonly": true,
          "items": {
            "$ref": "ListItem"
          }
        },
        "totalCount": {
          "title": "Number of Items",
          "type": "integer",
          "readonly": true
        },
        "totalPrice": {
          "title": "Total Price",
          "type": "string",
          "readonly": true
        },
        "emailBodyToSend": {
          "title": "Email Body",
          "type": "string",
          "readonly": true
        },
		"ExportedJSON": {
          "title": "Exported JSON",
          "type": "string",
          "readonly": true
        }
      }
    },
    "ListItem": {
      "id": "ListItem",
      "type": "object",
      "properties": {
        "productId": {
          "title": "Item ID",
          "type": "string"
        },
        "productName": {
          "title": "Item Name",
          "type": "string"
        },
        "productImage": {
          "title": "Item Image",
          "type": "string"
        },
        "productDescription": {
          "title": "Item Description",
          "type": "string"
        },
        "productPrice": {
          "title": "Item Unit Price",
          "type": "string"
        },
        "quantity": {
          "title": "Item Quantity",
          "type": "integer"
        },
        "totalPrice": {
          "title": "Item Total Price",
          "type": "string"
        }
      }
    }
  },
  "resources": {
    "ShoppingList": {
      "id": "ShoppingList",
      "if.interfaceAsset": "true",
      "title": "Shopping List",
      "description": "Add and remove priced items from a list.",
      "methods": {
        "InitTemplate": {
          "title": "Initialize email template",
          "description": "Retrieves an HTML Template from a given URL",
          "response": {
            "type": "none"
          },
          "parameters": {
            "templateURL": {
              "type": "string",
              "title": "Template URL",
              "required": true
            }
          }
        },
        "AddProductInfo": {
          "title": "Add Item",
          "description": "Adds an item to the shopping list using the following information.",
          "response": {
            "type": "none"
          },
          "parameters": {
            "productId": {
              "type": "string",
              "title": "Item ID",
              "required": true
            },
            "productName": {
              "type": "string",
              "title": "Item Name",
              "required": true
            },
            "productDescription": {
              "type": "string",
              "title": "Item Description",
              "required": true
            },
            "productImage": {
              "type": "string",
              "title": "Item Image",
              "required": true
            },
            "productPrice": {
              "type": "string",
              "title": "Item Unit Price",
              "required": true
            },
            "quantity": {
              "type": "integer",
              "title": "Quantity",
              "default": 1,
              "required": true
            }
          }
        },
        "clearList": {
          "title": "Clear list",
          "description": "Removes all items from the list.",
          "response": {
            "type": "none"
          }
        },
        "removeProduct": {
          "title": "Remove item",
          "description": "Removes the specified item from the list.",
          "response": {
            "type": "none"
          },
          "parameters": {
            "productId": {
              "type": "string",
              "title": "Item ID",
              "required": true
            }
          }
        },
        "incrementProductQuantity": {
          "title": "Increment quantity",
          "description": "Increments the quantity of the specified item by 1.",
          "response": {
            "type": "none"
          },
          "parameters": {
            "productId": {
              "type": "string",
              "title": "Item ID",
              "required": true
            }
          }
        },
        "decrementProductQuantity": {
          "title": "Decrement quantity",
          "description": "Decrements the quantity of the specified item by 1.",
          "response": {
            "type": "none"
          },
          "parameters": {
            "productId": {
              "type": "string",
              "title": "Item ID",
              "required": true
            }
          }
        },
        "setProductQuantity": {
          "title": "Set quantity",
          "description": "Adjusts the quantity of the specified item.",
          "response": {
            "type": "none"
          },
          "parameters": {
            "productId": {
              "type": "string",
              "title": "Item ID",
              "required": true
            },
            "newQuantity": {
              "type": "integer",
              "title": "Quantity",
              "default": 1,
              "required": true
            }
          }
        },
        "buildEmailBody": {
          "title": "Create email body",
          "description": "Creates a list summary that can be emailed using the Share Via Email interface assets.",
          "response": {
            "type": "none"
          }
        },
		 "buildJSONForExport": {
          "title": "Build JSON for Export",
          "description": "Creates a JSON version of this Shopping List.",
          "response": {
            "type": "none"
          }
        }
      },
      "events": {
        "MailBuilt": {
          "id": "MailBuilt",
          "title": "Mail created"
        },
        "ErrorReceived": {
          "id": "ErrorReceived",
          "title": "Error Received",
          "properties": {
            "errorMessage": {
              "title": "Message",
              "description": "The error message",
              "type": "string"
            }
          }
        }
      }
    }
  },
